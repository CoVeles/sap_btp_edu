PROCEDURE "get_supplier_info" (OUT result "HC450_OFFICESUPPLIES_SUPPLIERS") 
  LANGUAGE SQLSCRIPT 
  SQL SECURITY INVOKER 
  READS SQL DATA AS 
BEGIN tab_exc_us =
  SELECT *
  FROM "HC450_OFFICESUPPLIES_SUPPLIERS"
  WHERE "COUNTRY" != 'US';
  tab_us =
  SELECT *
  FROM "HC450_OFFICESUPPLIES_SUPPLIERS"
  WHERE "COUNTRY" = 'US';
  tab_all =
  SELECT *
  FROM :tab_exc_us
  UNION ALL
  SELECT *
  FROM :tab_us;
  result =
  SELECT *
  FROM :tab_all;
END